<?xml version="1.0"?>
<!DOCTYPE nmaprun>
<nmaprun scanner="nmap" args="nmap -oX - 192.168.1.1" start="..." version="7.94">
<host>
  <status state="up"/>
  <address addr="192.168.1.1" addrtype="ipv4"/>
  <ports>
    <port protocol="tcp" portid="22">
      <state state="open"/>
      <service name="ssh"/>
    </port>
    <port protocol="tcp" portid="80">
      <state state="open"/>
      <service name="http"/>
    </port>
  </ports>
</host>
</nmaprun>

